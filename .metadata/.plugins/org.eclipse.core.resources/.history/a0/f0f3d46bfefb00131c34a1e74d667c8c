package br.com.senac.view;

import javax.swing.JFrame;
import javax.swing.JLabel;
import java.awt.BorderLayout;
import javax.swing.ImageIcon;
import java.awt.Color;
import javax.swing.JPanel;

@SuppressWarnings("serial")
public class Principal extends JFrame {
	private JLabel lblNewLabel;
	private JPanel panelLog;

	public Principal() {
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setBounds(100, 100, 680, 448);
		
		lblNewLabel = new JLabel("");
		lblNewLabel.setBackground(Color.WHITE);
		lblNewLabel.setIcon(new ImageIcon(Principal.class.getResource("/Images/login.empresa.png")));
		getContentPane().add(lblNewLabel, BorderLayout.WEST);
		
		panelLog = new JPanel();
		getContentPane().add(panelLog, BorderLayout.CENTER);
		panelLog.setLayout(null);
	}

	public static void main(String[] args) {
		new Principal();
	}
}
